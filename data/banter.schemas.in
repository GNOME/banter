    <schema>
      <key>/schemas/apps/banter/group_windows/saved_windows</key>
      <applyto>/apps/banter/group_windows/saved_windows</applyto>
      <owner>banter</owner>
      <type>list</type>
      <default>[****Placeholder****]</default>
      <locale name="C">
         <short>Current saved list of Group Windows.</short>
         <long>
        List containing the GUIDs of the currently saved group windows.
        Please do not remove the ****Placeholder**** item.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/selected_theme</key>
      <applyto>/apps/banter/themes/selected_theme</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>Banter=Banter.theme</default>
      <locale name="C">
         <short>Selected Theme</short>
         <long>
	 	The name and path of the theme that should be used.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/valid_themes</key>
      <applyto>/apps/banter/themes/valid_themes</applyto>
      <owner>banter</owner>
      <type>list</type>
      <default>[Banter=Banter.theme]</default>
      <locale name="C">
         <short>List of valid themes</short>
         <long>
        List containing the name and paths of valid themes that are
        installed onto this system.  This should never be empty.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/selected_app_style</key>
      <applyto>/apps/banter/themes/selected_app_style</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>Banter=AppStyles/Banter</default>
      <locale name="C">
         <short>Selected App Style</short>
         <long>
	 	The name and path of the app style that is currently selected.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/valid_app_styles</key>
      <applyto>/apps/banter/themes/valid_app_styles</applyto>
      <owner>banter</owner>
      <type>list</type>
      <default>[Banter=AppStyles/Banter]</default>
      <locale name="C">
         <short>List of valid app styles</short>
         <long>
        List containing the name and paths of valid app styles that
        are installed onto this system.  This should never be empty.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/selected_contact_style</key>
      <applyto>/apps/banter/themes/selected_contact_style</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>Banter=ContactStyles/Banter</default>
      <locale name="C">
         <short>Selected Contact Style</short>
         <long>
	 	The name and path of the contact style that is currently selected.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/valid_app_styles</key>
      <applyto>/apps/banter/themes/valid_app_styles</applyto>
      <owner>banter</owner>
      <type>list</type>
      <default>[Banter=ContactStyles/Banter]</default>
      <locale name="C">
         <short>List of valid contact styles</short>
         <long>
        List containing the name and paths of valid contact styles that
        are installed onto this system.  This should never be empty.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/selected_message_style</key>
      <applyto>/apps/banter/themes/selected_message_style</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>PurePlastics=MessageStyles/PurePlastics.AdiumMessageStyle</default>
      <locale name="C">
         <short>Selected Message Style</short>
         <long>
	 	The name and path of the message style that is currently selected.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/valid_message_styles</key>
      <applyto>/apps/banter/themes/valid_message_styles</applyto>
      <owner>banter</owner>
      <type>list</type>
      <default>[PurePlastics=MessageStyles/PurePlastics.AdiumMessageStyle]</default>
      <locale name="C">
         <short>List of valid message styles</short>
         <long>
        List containing the name and paths of valid message styles that
        are installed onto this system.  This should never be empty.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/themes/selected_message_style_variant</key>
      <applyto>/apps/banter/themes/selected_message_style_variant</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>Blue vs Green</default>
      <locale name="C">
         <short>Selected Message Style Variant</short>
         <long>
	 	The name of the Message Style Variant that should be used.  Set this
	 	to an empty string to not use a variant.  The variant name depends on
	 	the selected message style.
         </long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
