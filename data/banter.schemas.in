
<gconfschemafile>
  <schemalist>

    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/server</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/server</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>talk.google.com</default>
      <locale name="C">
         <short>Server</short>
         <long>
		   Jabber server
         </long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/port</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/port</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>5223</default>
      <locale name="C">
         <short>Server Port</short>
         <long>
		   Jabber server port
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/tls</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/tls</applyto>
      <owner>banter</owner>
      <type>bool</type>
      <default>0</default>
      <locale name="C">
         <short>Use TLS</short>
         <long>
		   Use the TLS transport
         </long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/old-ssl</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/old-ssl</applyto>
      <owner>banter</owner>
      <type>bool</type>
      <default>1</default>
      <locale name="C">
         <short>Use ssl</short>
         <long>
		   Use old style ssl
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/auto-login</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/auto-login</applyto>
      <owner>banter</owner>
      <type>bool</type>
      <default>0</default>
      <locale name="C">
         <short>Auto Login</short>
         <long>
	 	Automatically login this account when rtc starts up
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/accounts/jabber/google-talk/im-connection-manager</key>
      <applyto>/apps/banter/accounts/jabber/google-talk/im-connection-manager</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>gabble.manager</default>
      <locale name="C">
         <short>Telepathy connection manager</short>
         <long>
	 	Telepathy connection manager with IM or Text capability for this account
         </long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/banter/accounts/sip/ekiga/server</key>
      <applyto>/apps/banter/accounts/sip/ekiga/server</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>ekiga.net</default>
      <locale name="C">
         <short>Server</short>
         <long>
		   SIP server
         </long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/banter/accounts/sip/ekiga/port</key>
      <applyto>/apps/banter/accounts/sip/ekiga/port</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>443</default>
      <locale name="C">
         <short>Server Port</short>
         <long>
		   SIP server port
         </long>
      </locale>
    </schema>

