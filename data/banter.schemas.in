      <key>/schemas/apps/banter/message_style/name</key>
      <applyto>/apps/banter/message_style/name</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>PurePlastics</default>
      <locale name="C">
         <short>Selected Message Style Name</short>
         <long>
	 	The name of the Message Style that should be used for the chat window.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/banter/message_style/variant_name</key>
      <applyto>/apps/banter/message_style/variant_name</applyto>
      <owner>banter</owner>
      <type>string</type>
      <default>Blue vs Green</default>
      <locale name="C">
         <short>Selected Message Style Variant</short>
         <long>
	 	The name of the Message Style Variant that should be used.  Set this
	 	to an empty string to not use a variant.  The variant name depends on
	 	the selected message style.
         </long>
      </locale>
    </schema>
